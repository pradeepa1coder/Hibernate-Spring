<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="myperson" class="org.jsp.Person">
		<property name="name" value="pradeep"></property>
		<property name="age" value="16"></property>
	</bean>

	<bean id="empid" class="org.jsp.Employee">
		<constructor-arg name="desg" value="Developer"></constructor-arg>
		<constructor-arg name="salary" value="30000"></constructor-arg>
	</bean>

</beans>